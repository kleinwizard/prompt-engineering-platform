{
  "name": "@prompt-platform/web",
  "version": "1.0.0",
  "description": "Next.js web application for Prompt Engineering Platform",
  "author": "Prompt Platform Team",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:prod": "NODE_ENV=production next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "export": "next build && next export",
    "clean": "rimraf .next out coverage node_modules/.cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:check": "next lint --max-warnings 0",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false --passWithNoTests",
    "test:update": "jest --updateSnapshot",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:test": "test-storybook",
    "build-storybook": "storybook build",
    "docker:build": "docker build -t prompt-platform-web .",
    "docker:run": "docker run -p 3000:3000 prompt-platform-web",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up web",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up web",
    "health": "curl -f http://localhost:3000/api/health || exit 1",
    "logs": "docker-compose logs -f web",
    "restart": "docker-compose restart web",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "analyze:bundle": "npx @next/bundle-analyzer",
    "precommit": "npm run lint:check && npm run type-check && npm run test:ci",
    "prebuild": "npm run clean",
    "postbuild": "echo 'Build completed successfully'",
    "validate": "npm run type-check && npm run lint:check && npm run test:ci"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^5.8.4",
    "@tiptap/extension-placeholder": "^2.1.13",
    "@tiptap/react": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "lodash": "^4.17.21",
    "lucide-react": "^0.292.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.5",
    "posthog-js": "^1.88.4",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hotkeys-hook": "^4.4.4",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.8",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.4",
    "tailwind-merge": "^2.0.0",
    "tailwindcss": "^3.3.6",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.3",
    "@storybook/addon-interactions": "^7.6.3",
    "@storybook/addon-links": "^7.6.3",
    "@storybook/addon-onboarding": "^1.0.9",
    "@storybook/blocks": "^7.6.3",
    "@storybook/nextjs": "^7.6.3",
    "@storybook/react": "^7.6.3",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-next": "^14.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "rimraf": "^3.0.2",
    "storybook": "^7.6.3",
    "typescript": "^5.2.2"
  }
}
